
public class Character {
	private int x,y;
	private int w,l,h;
	private int vx, vy;
	
	public Character(int x, int y){
		this.x = x;
		this.y = y;
	}

	public int getX() {
		return x;
	}

	public void setX(int x) {
		this.x = x;
	}

	public int getY() {
		return y;
	}

	public void setY(int y) {
		this.y = y;
	}
	
	public void moveRight(){
		x+=20;
	}
	public void moveLeft(){
		x-=20;
	}
		public int getVx() {
		return vx;
	}

	public void setVx(int vx) {
		this.vx = vx;
	}

	public int getVy() {
		return vy;
	}

	public void setVy(int vy) {
		this.vy = vy;
	}

	public void move(){
		x+=vx;
		y+=vy;
	}
	
	
	
}
